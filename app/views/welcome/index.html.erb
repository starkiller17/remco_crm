<header class="masthead  text-white text-center">
    <div class="container-fluid d-flex align-items-center flex-column">
        <!-- Masthead Avatar Image-->
        <%= image_tag("REMCO.png", :alt => "logo remco") %>
        <!-- Masthead Heading-->
        <h1 class="masthead-heading mb-0 text-dark">CRM REMCO</h1>
        <!-- Icon Divider-->
        <div class="divider-custom divider-dark">
            <div class="divider-custom-line"></div>
            <div class="divider-custom-icon">
                <svg width="1em" height="1em" viewBox="0 0 16 16" class="bi bi-star-fill" fill="currentColor" xmlns="http://www.w3.org/2000/svg">
                <path d="M3.612 15.443c-.386.198-.824-.149-.746-.592l.83-4.73L.173 6.765c-.329-.314-.158-.888.283-.95l4.898-.696L7.538.792c.197-.39.73-.39.927 0l2.184 4.327 4.898.696c.441.062.612.636.283.95l-3.523 3.356.83 4.73c.078.443-.36.79-.746.592L8 13.187l-4.389 2.256z"/>
                </svg>
            </div>
            <div class="divider-custom-line"></div>
        </div>
        <!-- Masthead Subheading-->
        <p class="masthead-subheading font-weight text-dark mb-0">Directorio de Clientes - Seguimiento de llamadas - Direcciones</p>
    </div>
</header>
<br/>
